{
  "name": "silvercommerce/geozones",
  "description": "Add generic 'geo-zones' a full list of country specific regions and the ability to categorise them into zones",
    "type": "silverstripe-vendormodule",
    "homepage": "https://github.com/silvercommerce/geozones",
    "keywords": [
        "silverstripe",
        "ISO 3166",
        "countries",
        "regions",
        "subdivisions",
        "zones"
    ],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Morven Lewis-Everley",
            "email": "morven@ilateral.co.uk"
        }
    ],
    "support": {
        "issues": "https://github.com/silvercommerce/geozones/issues"
    },
    "require": {
        "silverstripe/framework": "~4.0 || ^5",
        "silverstripe/siteconfig": "~4.0 || ^5"
    },
    "autoload": {
        "psr-4": {
            "SilverCommerce\\GeoZones\\": "src/",
            "SilverCommerce\\GeoZones\\Tests": "tests/"
        }
    },
    "extra": {
        "expose": [
            "client/dist"
        ]
    },
    "suggest": {
        "silverstripe/cms": "Add full content management support to your site"
    },
  "config": {
    "allow-plugins": {
      "composer/installers": true,
      "silverstripe/vendor-plugin": true
    },
    "sort-packages": true
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "squizlabs/php_codesniffer": "3.7"
  }
}
